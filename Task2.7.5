import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String s = sc.nextLine();

        switch (s) {
            case "Красный":
                System.out.println("Остановиться");
                break;
            case "Зеленый":
                System.out.println("Ехать");
                break;
            default:
                System.out.println("Подождать");
        }
    }
}
