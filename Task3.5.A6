public class Main {
   public static String string = "helloworld";

   public static void main(String[] args) {
       // code here...
       int n = string.length();
       System.out.println(string);
       
       for (int i = 0; i < n/2; i++) {
           string = string.substring(1, string.length()-1);
           System.out.println(string);
       }
   }
}
