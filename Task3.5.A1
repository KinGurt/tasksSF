import java.util.Arrays;

public class Main {
   public static int[] numbers = new int[100];

   public static void main(String[] args) {
        // code here...
        int j = 0;
        for (int i = -1; i >= -100; i--) {
            numbers[j] = i;
            j++;
        }

        System.out.println(Arrays.toString(numbers));
   }
}
